{
  "cli": {
    "version": ">= 15.0.15",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "prebuildCommand": "echo 'Using existing native folders'",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal",
      "prebuildCommand": "echo 'Using existing native folders'"
    },
    "production": {
      "autoIncrement": true,
      "prebuildCommand": "echo 'Using existing native folders'"
    }
  },
  "submit": {
    "production": {}
  }
}
